# Core:
CONFIG_SCHED_SMT=y

# Graphics:
CONFIG_MTRR=y
CONFIG_AGP=y
CONFIG_AGP_INTEL=y
CONFIG_DRM=y
CONFIG_DRM_FBDEV_EMULATION=y
CONFIG_DRM_I915=y

# Net:
CONFIG_R8169=y

# USB:
CONFIG_USB_XHCI_HCD=y
CONFIG_USB_HID=y
CONFIG_USB_STORAGE=y

# Sound:
CONFIG_SND_HDA_INTEL=y
CONFIG_SND_HDA_CODEC_REALTEK=y
CONFIG_SND_HDA_CODEC_HDMI=m

# CPU:
CONFIG_X86_INTEL_PSTATE=y
